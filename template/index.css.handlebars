@font-face {
  font-family: '{{ family }}';
  src: url('fonts/{{ familySlug }}-{{ nameSlug }}.eot');
  src: url('fonts/{{ familySlug }}-{{ nameSlug }}.eot?#iefix') format('embedded-opentype'),
       url('fonts/{{ familySlug }}-{{ nameSlug }}.woff') format('woff'),
       url('fonts/{{ familySlug }}-{{ nameSlug }}.ttf') format('truetype');
  font-weight: {{ weight }};
  font-style: {{ style }};
}